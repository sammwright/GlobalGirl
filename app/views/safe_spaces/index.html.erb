<p id="notice"><%= notice %></p>

<h1>Listing Safe Spaces</h1>

<table>
  <thead>
    <tr>
      <th>Contact name</th>
      <th>Contact phone</th>
      <th>Address</th>
      <th>Suburb</th>
      <th>State</th>
      <th>Postcode</th>
      <th>Country</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Contact email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @safe_spaces.each do |safe_space| %>
      <tr>
        <td><%= safe_space.contact_name %></td>
        <td><%= safe_space.contact_phone %></td>
        <td><%= safe_space.address %></td>
        <td><%= safe_space.suburb %></td>
        <td><%= safe_space.state %></td>
        <td><%= safe_space.postcode %></td>
        <td><%= safe_space.country %></td>
        <td><%= safe_space.latitude %></td>
        <td><%= safe_space.longitude %></td>
        <td><%= safe_space.contact_email %></td>
        <td><%= link_to 'Show', safe_space %></td>
        <td><%= link_to 'Edit', edit_safe_space_path(safe_space) %></td>
        <td><%= link_to 'Destroy', safe_space, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Safe space', new_safe_space_path %>
