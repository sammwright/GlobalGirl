<%= simple_form_for(@safe_space) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :contact_name %>
    <%= f.input :contact_phone %>
    <%= f.input :address %>
    <%= f.input :suburb %>
    <%= f.input :state %>
    <%= f.input :postcode %>
    <%= f.input :country %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :contact_email %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
